{
	"name": "headless-ui-kit",
	"version": "0.0.1",
	"scripts": {
		"cli:dev": "pnpm --filter cli dev",
		"cli:build": "pnpm --filter cli build",
		"cli:check": "pnpm --filter cli check",
		"cli:check:fix": "pnpm --filter cli check:fix",
		"prepare": "husky install",
		"pre-push": "lint-staged",
		"storybook": "BROWSER=none storybook dev -p 9009",
		"build:storybook": "storybook build",
		"test": "vitest"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.6.2",
	"dependencies": {
		"react": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@storybook/addon-essentials": "^8.6.7",
		"@storybook/addon-storysource": "^8.6.7",
		"@storybook/addon-webpack5-compiler-swc": "^3.0.0",
		"@storybook/manager-api": "^8.6.7",
		"@storybook/react": "^8.6.7",
		"@storybook/react-webpack5": "^8.6.7",
		"@storybook/theming": "^8.6.7",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.5.0",
		"vitest": "^3.0.9",
		"typescript": "catalog:"
	},
	"workspaces": ["packages/*"],
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json}": ["pnpm cli:check:fix"]
	}
}
